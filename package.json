  "description": "Just Another Text Editor Mark 2.  Couldn't get Mark 1 to fire off.",
  "main": "server.js",
  "scripts": {
    "start:dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "npm run server",
    "server": "nodemon server.js",
